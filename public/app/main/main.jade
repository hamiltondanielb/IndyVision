.container(data-ng-init="getLocations()")
  .row
    .col-lg-12
        google-map( center="map.center", zoom="map.zoom", draggable ="true", bounds="map.bounds", layers="maps.options")
            layers
                layer(type="TrafficLayer" show="true")
            markers
                marker(ng-repeat="ticket in map.tickets", coords="ticket.marker" options="ticket.options")
                marker(ng-repeat="accident in map.accidents", coords="accident.marker" options="accident.options")
                circle(center='Circle.center'
                radius='Circle.radius'
                fill='Circle.fill'
                stroke='Circle.stroke'
                clickable='Circle.cliclable'
                draggable='Circle.dragable'
                editable='Circle.editable'
                visible='Circle.visible')
        //input(type="text" ng-model="numberOfHoles") {{$scope.mapInstance.bounds.northeast.latitude}}
        //button(ng-click="getLocations()") Get Tickets

  .row
    .col-md-6
      div(ng-include="'/partials/main/featured-events'")
    .col-md-6
      div(ng-include="'/partials/main/new-events'")